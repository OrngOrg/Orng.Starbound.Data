<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>0.1.0</Version>
    <Authors>MiniOranges</Authors>
    <Company>OrngOrg</Company>
    <Description>Starbound Data Types</Description>
    <PackageProjectUrl>https://github.com/OrngOrg/Orng.Starbound.Data</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OrngOrg/Orng.Starbound.Data</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <DebugType>embedded</DebugType>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <RootNamespace>Star</RootNamespace>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
